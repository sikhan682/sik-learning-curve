{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
     "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "jsx": "react-native",
    "types": ["nativewind/types", "webpack-env"]
  },
  "include": ["expo-plugins", "src", "*.ts", "**/*.js", "index.tsx"],
  "exclude": ["node_modules"]
  }
}
